      <script>
        $(document).ready(function() {
          $(document).foundation();
          MyAngelcam.messagesModule = new MyAngelcam.messages;
          MyAngelcam.Analytics = new MyAngelcam.AnalyticsModule();
        });
      </script>
      
<script>
  $(function(){

    var progressBarContainer = $('<div>');
    $('#header').after(progressBarContainer);

    var progressBar = new MyAngelcam.ProgressBar(progressBarContainer, {
      steps: 4
    });

    
      MyAngelcam.Analytics.trackPageView('/accounts/signup/');
    

    
      var nextFieldValue = $('#id_next').val();
      if (nextFieldValue !== '/dashboard/') {
        // if next url is present, append it to redirect url (for more info, see SignupForm)
        $('#id_next').val(nextFieldValue + '?next=/dashboard/');
      }
    

    if ($('input[name="account_type_selector"]').is(':checked')) {
      $('#account_type_selector_submit').removeClass('disabled');
    }

    $('#account_type_selector_submit').on('click', function(e) {
      if ($(this).hasClass('disabled')) {
        return;
      }
      var selectedAccountType = $('input[name="account_type_selector"]:checked').val();
      $('#id_business_type').val(selectedAccountType);
      window.history.pushState(null, document.title, window.location.href);
      prepareFormForBusinessType(selectedAccountType);
      $('#account-type-content').fadeOut(function() {
        $('#account-create-content').fadeIn();
      });
    });
    $('input[name="account_type_selector"]').on('change', function() {
      $('#account_type_selector_submit').removeClass('disabled');
    });

    function prepareFormForBusinessType(type) {
      $('body').removeClass('initial');
      $('[data-hide="' + type + '"]').hide();
      if (type === 'business') {
        progressBar.step(2);
        // As default label is "E-mail" we change label for business acc
        $('label[for="id_email"]').text('Business E-mail*');
      } else {
        progressBar.hide();
      }
      MyAngelcam.Analytics.trackPageView('/accounts/signup/' + type + '/');
    }

    function goBackToTypeSelect() {
      progressBar.show();
      progressBar.step(1);
      $('body').addClass('initial');
      $('#account-create-content').fadeOut(function() {
        $('#account-type-content').fadeIn();
        $('[data-hide]').show();
        // Change label of input back to original value
        $('label[for="id_email"]').text('E-mail*');
        MyAngelcam.Analytics.trackPageView('/accounts/signup/');
      });
    }

    window.onpopstate = function(event) {
      goBackToTypeSelect();
    };

    new MyAngelcam.phoneInput($('#id_phone'));
  });
</script>

    </body>
</html>
